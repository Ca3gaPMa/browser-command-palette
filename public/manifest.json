{
    "name": "Browser Command Palette",
    "description": "Powerful command palette for efficient browsing.",
    "version": "0.0.1",
    "manifest_version": 3,
    "action": {
        "default_popup": "index.html",
        "default_title": "Open command palette."
    },
    "icons": {
        "16": "logo192.png",
        "48": "logo192.png",
        "128": "logo192.png"
    },
    "permissions": ["activeTab", "tabs", "system.display", "system.storage", "system.cpu"],
    "content_scripts": [
        {
            "matches": ["http://*/*", "https://*/*"],
            "js": ["./static/js/content.js"]
        }
    ]
}
