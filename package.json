        "format": "prettier --write \"**/*.+(js|ts|json)\"",
        "fixint": "eslint src --fix"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
