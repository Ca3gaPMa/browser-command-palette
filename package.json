{
    "name": "chrome-command-palette",
    "version": "0.1.0",
    "dependencies": {
        "@craco/craco": "^7.0.0-alpha.3",
        "@emotion/react": "^11.9.3",
        "@emotion/styled": "^11.9.3",
        "@mui/material": "^5.9.2",
        "@types/jest": "^27.5.2",
        "@types/node": "^16.11.45",
        "@types/react": "^18.0.15",
        "@types/react-dom": "^18.0.6",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-scripts": "5.0.1",
        "typescript": "^4.7.4",
        "web-vitals": "^2.1.4"
    },
    "devDependencies": {
        "@types/chrome": "^0.0.193",
        "@typescript-eslint/eslint-plugin": "^5.30.7",
        "@typescript-eslint/parser": "^5.30.7",
        "eslint": "^8.20.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.2.1",
        "prettier": "2.7.1"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "npm run format && set INLINE_RUNTIME_CHUNK=false && craco build",
